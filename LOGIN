package NEW;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;

public class slnmclass {

	public static void main(String[] args) {
		System.setProperty("webdriver.chrome.driver", "D:\\Users\\surendra\\Downloads\\seleniumtest\\Chromedriver-win64\\chromedriver-win64\\chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		driver.get("http://216.48.184.162/OmniRetailer/");
		
		WebElement userIdField = driver.findElement(By.id("userId"));
        WebElement emailIdField = driver.findElement(By.id("emailId"));
        WebElement passwordField  = driver.findElement(By.id("password"));
        
        userIdField.sendKeys("CID8995438");
        emailIdField.sendKeys("support@retailclouds.com");
        passwordField.sendKeys("Password123#");
        WebElement login = driver.findElement(By.cssSelector(".btn.bg-olive.btn-block"));
        login.click();
		// TODO Auto-generated method stub

	}

}
